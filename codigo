#include <stdio.h>
#include <iostream>

using namespace std;
int main (void)
{
	int i, j, k;
	
	for(i=1; i<=12; i++)
	{
		cout<< "  Tabla del  " << i << "\n";
		for(j=1; j<=12; j++)
		{
			k=i*j;
			cout<< i <<"  x  "<< j << "  =  " << k << "\n";
		}
		cout<< "\n\n\n";
	}
cout<<"\n\n\n";
system("PAUSE");
return 0;

}
